{
    "name": "inetstudio/blueprint",
    "type": "project",
    "description": "Blueprint",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "barryvdh/laravel-debugbar": "^3.5.1",
        "doctrine/dbal": "^3.0",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "inetstudio/admin-panel": "^8.0",
        "inetstudio/checks-contest": "^8.0",
        "inetstudio/feedback": "^8.0",
        "inetstudio/pages": "^8.0",
        "inetstudio/sitemap": "^8.0",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "mcaskill/composer-exclude-files": "^2.0",
        "sentry/sentry-laravel": "^2.1.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.5.0",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "exclude-from-files": [
            "elasticsearch/elasticsearch/src/autoload.php"
        ],
        "laravel": {
            "dont-discover": [
                "inetstudio/acl",
                "inetstudio/addresses",
                "inetstudio/admin-panel",
                "inetstudio/cache",
                "inetstudio/checks-contest",
                "inetstudio/captcha",
                "inetstudio/classifiers",
                "inetstudio/feedback",
                "inetstudio/fns",
                "inetstudio/meta",
                "inetstudio/pages",
                "inetstudio/search",
                "inetstudio/simple-counters",
                "inetstudio/sitemap",
                "inetstudio/uploads",
                "inetstudio/widgets",

                "barryvdh/laravel-debugbar",
                "barryvdh/laravel-ide-helper",
                "beyondcode/laravel-dump-server",
                "cviebrock/eloquent-sluggable",
                "fideloper/proxy",
                "laravel/tinker",
                "laravelcollective/html",
                "laravelium/sitemap",
                "maatwebsite/excel",
                "sentry/sentry-laravel",
                "tightenco/ziggy",
                "yajra/laravel-datatables-buttons",
                "yajra/laravel-datatables-fractal",
                "yajra/laravel-datatables-html",
                "yajra/laravel-datatables-oracle"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Packages\\FeedbackPackage\\Feedback\\": "packages/feedback/entities/feedback/src",
            "Packages\\ReceiptsContest\\Receipts\\": "packages/receipts-contest/entities/receipts/src",
            "Packages\\ReceiptsContest\\Products\\": "packages/receipts-contest/entities/products/src",
            "Packages\\MainPagePackage\\MainPage\\": "packages/mainpage/entities/mainpage/src",
            "Packages\\MetaPackage\\Meta\\": "packages/meta/entities/meta/src",
            "Packages\\PagesPackage\\Pages\\": "packages/pages/entities/pages/src",
            "Packages\\Uploads\\": "packages/uploads/entities/uploads/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
