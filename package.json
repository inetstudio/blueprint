{
    "private": true,
    "scripts": {
        "test": "npm run development",
        "dev": "npm run development",
        "prod": "npm run production",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "production": "mix --production",
        "front-dev": "npm run front-development",
        "front-test": "npm run front-development",
        "front-prod": "npm run front-production",
        "front-development": "mix --mix-config=resources/assets/front/webpack.mix.js",
        "front-watch": "mix watch --mix-config=resources/assets/front/webpack.mix.js",
        "front-hot": "mix watch --hot --mix-config=resources/assets/front/webpack.mix.js",
        "front-watch-poll": "mix watch --mix-config=resources/assets/front/webpack.mix.js -- --watch-options-poll=1000",
        "front-production": "mix --production --mix-config=resources/assets/front/webpack.mix.js"
    },
    "devDependencies": {},
    "workspaces": [
        "vendor/inetstudio/*"
    ]
}
